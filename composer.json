{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/artjomsimon/Foundation"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "pomm-project/cli": "^2.0",
        "pomm-project/foundation": "dev-master",
        "pomm-project/model-manager": "^2.0",
        "symfony/var-dumper": "^4.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
